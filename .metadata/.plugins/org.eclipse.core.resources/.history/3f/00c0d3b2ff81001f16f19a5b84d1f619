package control.loop;

public class Gugu {
	public static void main(String[] args) {
		
		for (int i = 2 ; i <=9 ; i=i+4) {
			for(int j =1 ; j <=4 ; j++){
				System.out.print(i + "*" + j + "=" + i*j+"\t");
				System.out.print(i + "*" + j + "=" + i*j+"\t");
			}
			System.out.println();
		}
		System.out.println("end");
	}
}
