package control;

import java.util.Scanner;

public class SwitchTest2 {
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		System.out.print("n : ");

		
		switch(n%2) {
		case 1:
			System.out.println("남자");
			break;
		case 0:
			System.out.println("여자");
			break;
		default:
			System.out.println("에러");
			break;
		}
		System.out.println("종료");
	}
}
